{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../source/semantic/",
      "uncompressed": "../source/semantic/components/",
      "compressed": "../source/semantic/components/",
      "themes": "../source/semantic/themes/"
    },
    "clean": "../source/semantic/"
  },
